<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

	<bean id="mojLogger" class="info.owczarek.springtutorial.implementations.LoggerImpl"></bean>

	<bean id="repozytoriumUzytkownikow"
		class="info.owczarek.springtutorial.implementations.UsersRepositoryImpl">
		<property name="logger" ref="mojLogger"></property>
	</bean>

</beans>
